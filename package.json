    "**/@ethersproject/web": "^5.8.0",
    "**/@ethersproject/wordlists": "^5.8.0",
    "**/@openzeppelin/contracts": "^4.9.6",
    "**/@openzeppelin/contracts-upgradeable": "^4.9.6",
    "**/axios": "^0.30.0",
    "**/cookie": "^0.7.2",
    "**/elliptic": "^6.6.1"
  }
}
